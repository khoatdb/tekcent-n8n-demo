<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <!--
    Release configuration transformations for Sitecore 10.4
  -->

  <!-- Set debug mode to false -->
  <system.web>
    <compilation xdt:Transform="SetAttributes" debug="false" />
    <customErrors xdt:Transform="SetAttributes" mode="RemoteOnly" />
  </system.web>

  <!-- Production settings -->
  <appSettings>
    <add key="sitecore:define" value="TRACE" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
  </appSettings>
</configuration>